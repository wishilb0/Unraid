<?xml version="1.0"?>
<Container version="2">
  <Name>FileBrowser</Name>
  <Repository>filebrowser/filebrowser</Repository>
  <Registry>https://hub.docker.com/r/filebrowser/filebrowser</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/78481-support-knex666-filebrowser/</Support>
  <Project>https://github.com/filebrowser/filebrowser</Project>
  <Overview/>
  <Category>Cloud: Network:Web</Category>
  <WebUI>http://[IP]:[PORT:80]/files/</WebUI>
  <TemplateURL>https://raw.githubusercontent.com/maschhoff/docker/master/unraid%20xml/my-filebrowser.xml</TemplateURL>
  <Icon>http://192.168.1.100:8089/uploads/2024/09/05/Filebrowser_A.png</Icon>
  <ExtraParams/>
  <PostArgs>-d /db/database.db</PostArgs>
  <CPUset/>
  <DateInstalled>1725507545</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Host Path 1" Target="/srv/appdata" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/appdata/</Config>
  <Config Name="Host Path 2" Target="/db/" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/appdata/FileBrowser/data</Config>
  <Config Name="Host Path 3" Target="/srv/Mydata" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/Mydata/</Config>
  <Config Name="Host Path 4" Target="/srv/Backup" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/disk1/Backup/</Config>
  <Config Name="Host Path 5" Target="/srv/media" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/disk2/media</Config>
  <Config Name="Host Port 1" Target="80" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">8082</Config>
  <Config Name="/dev/dri" Target="" Default="" Mode="" Description="" Type="Device" Display="always" Required="false" Mask="false">/dev/dri</Config>
  <Config Name="Host Path 6" Target="/srv/build-images" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/cache/build-images/</Config>
</Container>
